{
    "workbench.startupEditor": "none",
    "security.workspace.trust.untrustedFiles": "open",
    "cmake.showOptionsMovedNotification": false,
    "vs-kubernetes": {
        "vscode-kubernetes.kubectl-path-linux": "/home/anthony/.local/state/vs-kubernetes/tools/kubectl/kubectl",
        "vscode-kubernetes.helm-path-linux": "/home/anthony/.local/state/vs-kubernetes/tools/helm/linux-amd64/helm",
        "vscode-kubernetes.minikube-path-linux": "/home/anthony/.local/state/vs-kubernetes/tools/minikube/linux-amd64/minikube"
    },
    "redhat.telemetry.enabled": true,
    "editor.fontFamily": "'InputMono Nerd Font', 'Input Mono', 'Droid Sans Mono', 'monospace', monospace",
    "editor.fontSize": 14,
    "editor.lineHeight": 25,
    "workbench.iconTheme": "Monokai Pro (Filter Spectrum) Icons",
    "[sql]": {
        "editor.defaultFormatter": "mtxr.sqltools"
    },
    "[cpp]": {
        "editor.defaultFormatter": "xaver.clang-format"
    },
    "javascript.preferences.quoteStyle": "double",
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "solidity.telemetry": false,
    "git.openRepositoryInParentFolders": "never",
    "kotlin.languageServer.enabled": true,
    "kotlin.debugAdapter.enabled": false,
    "files.autoSave": "afterDelay",
    "editor.formatOnPaste": true,
    "java.import.generatesMetadataFilesAtProjectRoot": true,
    "files.exclude": {
        "**/.project": true,
        "**/.classpath": true,
        "**/.settings": true
    },
    "hardhat.telemetry": true,
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[python]": {
        "editor.formatOnType": true
    },
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "workbench.colorTheme": "Monokai Pro (Filter Spectrum)",
    "github.copilot.chat.agent.thinkingTool": true,
    "terminal.external.linuxExec": "kitty",
    "terminal.integrated.drawBoldTextInBrightColors": false,
    "python.defaultInterpreterPath": "/etc/profiles/per-user/anthony/bin/python",
    "update.mode": "none",
    "nixEnvSelector.useFlakes": true,
    "docker.extension.enableComposeLanguageServer": false,
    "@azure.argTenant": "",
    "editor.unicodeHighlight.includeStrings": false,
    "workbench.editor.autoLockGroups": {
        "mainThreadWebview-markdown.preview": true
    },
    "editor.fontLigatures": false,
    "qt-core.additionalQtPaths": [
        {
            "name": "Qt-6.9.3-linux-g++_from_PATH",
            "path": "/etc/profiles/per-user/anthony/bin/qtpaths"
        },
        {
            "name": "Qt-6.9.3-linux-g++",
            "path": "/etc/profiles/per-user/anthony/bin/qmake"
        }
    ],
    "qt-core.defaultProjectDirectory": "/home/anthony/dev/qt-projects",
    "qt-qml.doNotAskForQmllsDownload": true
}
